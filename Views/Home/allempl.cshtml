@model IEnumerable<empl>

<html>
	<head>
		<title>all emploeey</title>
		<style>
			table.table-hover tbody tr:hover{
				background-color:orange;
			}
			td:hover {
				background-color:greenyellow;
			}
		</style>
	</head>
	<body>

		<h2>all emoloeey page</h2>
		<table class="table table-bordered table-striped table-hover">
			<thead>
				<tr>
					<th>name</th>

					<th>depart</th>
				<th>birthday</th>
				<th>email</th>
				</tr>
			</thead>
			<tbody>
			@foreach (empl emp in Model)
			{
				<tr>
					<td>@emp.Name</td>
					<td>@emp.depart</td>
					<td>@emp.birthdate.ToShortDateString()</td>
					<td>@emp.email</td>
				</tr>
			}
		</tbody>
		</table>

	</body>
</html>
